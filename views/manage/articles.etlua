<table class="table table-hover">
    <thead>
      <tr>
        <th> Title </th>
        <th> Author </th>
        <th> Date </th>
        <th></th>
      </tr>
      <% for i, article in pairs(articles) do %>
      <tr>
        <td> <%= article.title %> </td>
        <td> <%= article:get_author().name %> </td>
        <td> <%= article.date %> </td>
        <td><a href="/admin/article/<%= article.id %>"> Edit </a></td>
      </tr>
      <% end %>
    </thead>
  </table>