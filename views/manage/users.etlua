  <table class="table table-hover">
    <thead>
      <tr>
        <th> Name </th>
        <th> Email </th>
      </tr>
      <% for i, author in pairs(authors) do %>
      <tr>
        <td> <%= author.name %> </td>
        <td> <%= author.email %> </td>
      </tr>
      <% end %>
    </thead>
  </table>